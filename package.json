{
  "name": "@demacia/cmjs-lib",
  "version": "0.0.1",
  "description": "angular common js lib",
  "scripts": {
    "build": "ng-packagr -p package.json",
    "publish": "npm publish dist --access public",
    "clean": "rimraf dist",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "doc": "docsify serve docs --open"
  },
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "ngPackage": {
    "lib": {
      "entryFile": "public_api.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/zw277856645/cmjs-lib"
  },
  "keywords": [
    "angular",
    "common",
    "lib",
    "animate",
    "animation",
    "decorator",
    "util",
    "service"
  ],
  "author": "wei.zhang",
  "license": "MIT",
  "peerDependencies": {
    "@angular/core": "^8.1.2",
    "@angular/animations": "^8.1.2",
    "@angular/platform-browser-dynamic": "^8.1.2",
    "rxjs": "^6.5.1",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular/compiler": "8.1.2",
    "@angular/compiler-cli": "8.1.2",
    "@angular/core": "8.1.2",
    "@angular/animations": "8.1.2",
    "@types/node": "10.14.5",
    "core-js": "2.4.1",
    "zone.js": "0.9.1",
    "ng-packagr": "5.1.0",
    "typescript": "3.4.5",
    "rimraf": "2.6.1",
    "rxjs": "6.5.1",
    "tslint": "5.11.0",
    "rxjs-tslint": "0.1.7",
    "codelyzer": "5.1.0"
  }
}
